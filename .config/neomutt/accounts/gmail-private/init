set my_gmail_private_data = "$my_documents_dir/accounts/gmail-private.gpg"
set my_gmail_private_address = "`decget \"$my_gmail_private_data\" user`"
set my_gmail_private_smtp_pass = "`decget \"$my_gmail_private_data\" app-password`"
set my_gmail_private_sign_keyid = "`decget \"$my_gmail_private_data\" pgp-sign-keyid`"
set my_gmail_private_self_encrypt_keyid = "`decget \"$my_gmail_private_data\" pgp-self-encrypt-keyid`"

set my_gmail_private_source = enter
source enter

